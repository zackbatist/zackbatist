@article{ocallaghan2021,
  title = {\#{{MeToo Online Disclosures}}: {{A Survivor-Informed Approach}} to {{Open Science Practices}} and {{Ethical Use}} of {{Social Media Data}}},
  shorttitle = {\#{{MeToo Online Disclosures}}},
  author = {O’Callaghan, Erin and Douglas, Hannah M.},
  date = {2021-12-01},
  journaltitle = {Psychology of Women Quarterly},
  volume = {45},
  number = {4},
