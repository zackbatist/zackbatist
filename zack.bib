@book{garstki2022,
  title = {Critical {{Archaeology}} in the {{Digital Age}}: {{Proceedings}} of the 12th {{IEMA Visiting Scholar}}’s {{Conference}}. {{Location}}: {{The Cotsen Institute}} of {{Archaeology Press}}},
