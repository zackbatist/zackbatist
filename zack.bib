@article{diaz-guardamino2019,
  title = {Human, {{Transhuman}}, {{Posthuman Digital Archaeologies}}: {{An Introduction}}},
  shorttitle = {Human, {{Transhuman}}, {{Posthuman Digital Archaeologies}}},
  author = {Díaz-Guardamino, Marta and Morgan, Colleen},
  date = {2019-08},
  journaltitle = {European Journal of Archaeology},
  volume = {22},
  number = {3},
  pages = {320--323},
  issn = {1461-9571, 1741-2722},
  doi = {10.1017/eaa.2019.26},
  url = {https://www.cambridge.org/core/journals/european-journal-of-archaeology/article/human-transhuman-posthuman-digital-archaeologies-an-introduction/9E998CD5CB3CCFF0AA14ED32A78ED14D},
  urldate = {2019-07-26},
  abstract = {Current archaeological thought evokes a sparking Catherine wheel: spinning fireworks that detonate light, colour, and sound with every movement. These theoretical turns swirl alongside the ongoing development and adoption of scientific and digital techniques that have wide-ranging implications for archaeological practices and interpretations. Two particularly combustible developments are posthumanism and the ontological turn, which emerged within the broader humanities and social sciences. Posthumanism rejects human exceptionalism and seeks to de-centre humans in archaeological discourse and practice. Linked to this is the so-called ‘ontological turn’ (aka the ‘material turn’), a shift away from framing archaeological research within a Western ontology and a movement beyond representationalism (i.e. focusing on things themselves rather than assuming that objects represent something else).},
  langid = {english},
  timestamp = {2019-07-26T01:24:27Z},
  file = {Díaz-Guardamino & Morgan 2019 - Human, Transhuman, Posthuman Digital Archaeologies.pdf}
}

