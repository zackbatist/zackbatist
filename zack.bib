@article{dourish2014,
  title = {No SQL: The Shifting Materialities of Database Technology : Computational Culture},
  shorttitle = {No SQL},
  author = {Dourish, Paul},
  date = {2014-11-09},
  journaltitle = {Computational Culture},
  number = {4},
