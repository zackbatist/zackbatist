@article{mcguire2021,
  title = {A {{Relational Marxist Critique}} of {{Posthumanism}} in {{Archaeology}}},
  author = {McGuire, Randall H.},
  date = {2021-08},
  journaltitle = {Cambridge Archaeological Journal},
  volume = {31},
  number = {3},
  pages = {495--501},
  publisher = {Cambridge University Press},
  issn = {0959-7743, 1474-0540},
  doi = {10.1017/S0959774321000184},
  url = {https://www.cambridge.org/core/journals/cambridge-archaeological-journal/article/abs/relational-marxist-critique-of-posthumanism-in-archaeology/714FFAF76C22540E867B5775EFC1905B},
  urldate = {2021-08-30},
  abstract = {In archaeology, a Posthumanism has arisen from the ashes of post-modernism and declared that Marxism is dead in archaeology. Archaeological advocates of the posthumanist theory of Symmetrical Archaeology cherry-pick ideas to dismiss Marxism out of hand without considering the depth and nuances of different Marxist theories. They misrepresent the relational dialectic as oppositional thinking and ignore the fundamental dualism of their own polemic. They equate humans and things by arguing that they share a common ontology. A relational Marxism resolves the dualistic nature of their polemic and shows that things, animals and people may be studied relationally while still recognizing ontological differences. Marxism lives.},
  langid = {english},
