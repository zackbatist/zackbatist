@article{maitner2024,
  title = {Code Sharing in Ecology and Evolution Increases Citation Rates but Remains Uncommon},
  author = {Maitner, Brian and Santos Andrade, Paul Efren and Lei, Luna and Kass, Jamie and Owens, Hannah L. and Barbosa, George C. G. and Boyle, Brad and Castorena, Matiss and Enquist, Brian J. and Feng, Xiao and Park, Daniel S. and Paz, Andrea and Pinilla-Buitrago, Gonzalo and Merow, Cory and Wilson, Adam},
  date = {2024},
  journaltitle = {Ecology and Evolution},
  volume = {14},
