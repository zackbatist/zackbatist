@article{schofield2017,
  title = {‘{{Deviants}}, Punks and {{Pink Fairies}}’: Counter-Archaeologies for Unreasonable People},
  shorttitle = {‘{{Deviants}}, Punks and {{Pink Fairies}}’},
  author = {Schofield, John},
  date = {2017-05-27},
  journaltitle = {World Archaeology},
  volume = {49},
  number = {3},
  pages = {281--290},
  issn = {0043-8243},
  doi = {10.1080/00438243.2017.1383182},
  url = {https://doi.org/10.1080/00438243.2017.1383182},
  urldate = {2017-12-05},
  abstract = {Like any subject, archaeology has its traditions, its conventional practices and guiding principles that ensure it stays true to its original aims. That is why archaeologists still excavate, generating data, and publishing in the latest issues of long-established journals or in expensive monographs that only other archaeologists will read. Archaeologists employed by heritage agencies care for monuments and historic buildings, ensuring they survive, ‘for the benefit of future generations’. For many, this is how archaeology should remain. A similar cultural conservatism lingered in the UK from the end of the Second World War until, arguably, 26 November 1976 when, according to musician Phil Oakey, the Second World War finally came to an end. That was the day punk arrived with the thrashing of guitars and streams of abuse. But punk was about more than just the music. It was an ethos, a philosophy. Conventions were challenged and the old order gradually dismantled. Pre-1976 news footage now has the semblance of ancient history. This semi-autobiographical introduction to a collection of essays that promotes new thinking on counter-archaeologies wonders if that punk ethos could not have similar benefits for archaeology. Or perhaps it exists already, more than we care to realize.},
  file = {Schofield 2017 - ‘Deviants, punks and Pink Fairies’.pdf}
}

@article{schofield2020,
  title = {Integrating Geophysical Survey and Excavation at the {{Freston Early Neolithic}} Causewayed Enclosure, {{Suffolk}} ({{UK}})},
  author = {Schofield, Tim and Carter, Tristan and Jackson, Nathaniel and Moir, Rose},
  date = {2020},
  journaltitle = {Archaeological Prospection},
  volume = {n/a},
  number = {n/a},
  issn = {1099-0763},
  doi = {10.1002/arp.1800},
  url = {http://onlinelibrary.wiley.com/doi/abs/10.1002/arp.1800},
  urldate = {2020-11-25},
  abstract = {A 2019 geophysical survey of part of the 8.55-ha Freston interrupted ditch system (Suffolk), which integrated reconnaissance fluxgate gradiometry with earth resistance metre techniques, produced a fine-grained geo-spatial resolution site plan to undertake a pilot-study research excavation. The features targeted comprised four ditch termini of the enclosure, with the aim being to test the long-established hypothesis that the monument represented an Early Neolithic causewayed enclosure. Although the geophysical survey successfully located the ditch termini and linear trends indicative of the internal palisade trench, the magnetic data in particular further suggested the presence of numerous pit-like anomalies both within and outside of the enclosure. The combined survey data also detailed for the first time an approximately 70-m-long curvilinear low-resistance ditch-like anomaly that appears to precede the interrupted ditch system. The working hypothesis is that this represents a ditch flanking a long barrow, a type of communal Early Neolithic mortuary monument that typically predate the construction of causewayed enclosures across southern Britain. The subsequent 6-week excavation achieved its aims by recovering diagnostic Early Neolithic (earlier 4th millennium cal BC) material culture from the ditch fills.},
  langid = {english},
  file = {Schofield et al 2020 - Integrating geophysical survey and excavation at the Freston Early Neolithic causewayed enclosure, Suffolk (UK).pdf}
}

