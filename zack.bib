@article{fotiadis1993,
  title = {Regions of the {{Imagination}}: {{Archaeologists}}, {{Local People}}, and the {{Archaeological Record}} in {{Fieldwork}}, {{Greece}}},
  shorttitle = {Regions of the {{Imagination}}},
  author = {Fotiadis, Michael},
  date = {1993-09-01},
