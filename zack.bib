  urldate = {2022-10-19},
  abstract = {Despite their rapid proliferation, there has been little examination of the coordination and social practices of cyberinfrastructure projects. We use the notion of "human infrastructure" to explore how human and organizational arrangements share properties with technological infrastructures. We conducted an 18-month ethnographic study of a large-scale distributed biomedical cyberinfrastructure project and discovered that human infrastructure is shaped by a combination of both new and traditional team and organizational structures. Our data calls into question a focus on distributed teams as the means for accomplishing distributed work and we argue for using human infrastructure as an alternative perspective for understanding how distributed collaboration is accomplished in big science.},
  isbn = {978-1-59593-249-5}
}

