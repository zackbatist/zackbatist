  url = {https://www.cambridge.org/core/journals/american-antiquity/article/modeling-relationships-between-space-movement-and-lithic-geometric-attributes/0A758BEC8978627BC11CFDF5BFEAA02D?utm_source=SFMC&utm_medium=email&utm_content=https%3a%2f%2fwww.cambridge.org%2fcore%2fjournals%2famerican-antiquity%2farticle%2fmodeling-relationships-between-space-movement-and-lithic-geometric-attributes%2f0A758BEC8978627BC11CFDF5BFEAA02D&utm_campaign=KPT_SAA_Oct+2018_+20181009_154854&WT.mc_id=KPT_SAA_Oct+2018_+20181009_154854},
