  langid = {english},
  pagetotal = {326}
}

@book{strathern2004,
  title = {Commons and {{Borderlands}}: {{Working Papers}} on {{Interdisciplinarity}}, {{Accountability}} and the {{Flow}} of {{Knowledge}}},
