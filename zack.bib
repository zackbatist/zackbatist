@incollection{strauss1993,
  title = {Negotiated {{Order}} and {{Structural Ordering}}},
  booktitle = {Continual {{Permutations}} of {{Action}}},
  author = {Strauss, Anselm L. and Maines, David R.},
  date = {1993},
  publisher = {Routledge},
  abstract = {This conclusion presents some closing thoughts on the concepts discussed in the preceding chapters of this book. The book outlines the Pragmatist/interactionist position on order and change, including the extension of it through the concept of "negotiated order". It demonstrates some implications of an interactionist theory of action for extending still further the position represented by what has come to be called "the negotiated-order approach". After a deliberately autobiographical introduction to this Pragmatist-derived interactionist theory of action (acting), a lengthy list of assumptions behind that theory was presented. These assumptions are very useful for understanding the Chicago interactionist tradition as it has been influenced by Pragmatism, and lead to a theory of action. The book also includes discussion on work and its relations, the body, thought processes, symbolizing, social worlds and arenas, representation, the interplay of routine, and the relevance of the concept of social worlds and studying contemporary society.},
