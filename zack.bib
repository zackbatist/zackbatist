@article{ferreira2013,
  title = {On the Usefulness of Ontologies in Epidemiology Research and Practice},
  author = {Ferreira, João D. and Paolotti, Daniela and Couto, Francisco M. and Silva, Mário J.},
  date = {2013-05-01},
  journaltitle = {J Epidemiol Community Health},
  shortjournal = {J Epidemiol Community Health},
  volume = {67},
  number = {5},
