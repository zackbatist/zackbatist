@article{okumura2019,
  title = {Archaeology, Biology, and Borrowing: {{A}} Critical Examination of {{Geometric Morphometrics}} in {{Archaeology}}},
  shorttitle = {Archaeology, Biology, and Borrowing},
  author = {Okumura, Mercedes and Araujo, Astolfo G. M.},
  date = {2019-01-01},
  journaltitle = {Journal of Archaeological Science},
  shortjournal = {Journal of Archaeological Science},
  volume = {101},
