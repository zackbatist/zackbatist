@article{hein2017,
  title = {Automated Classification of Archaeological Ceramic Materials by Means of Texture Measures},
  author = {Hein, Irmgard and Rojas-Domínguez, Alfonso and Ornelas, Manuel and D'Ercole, Giulia and Peloschek, Lisa},
  date = {2017-12-27},
  journaltitle = {Journal of Archaeological Science: Reports},
