@article{renear2009,
  title = {Strategic {{Reading}}, {{Ontologies}}, and the {{Future}} of {{Scientific Publishing}}},
  author = {Renear, Allen H. and Palmer, Carole L.},
  date = {2009-08-14},
  journaltitle = {Science},
  volume = {325},
  number = {5942},
  eprint = {19679805},
